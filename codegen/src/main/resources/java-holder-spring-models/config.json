{
  "globalVariables": {
    "basePackageName": "au.org.consumerdatastandards.holder.model",
    "basePackagePath": "au/org/consumerdatastandards/holder/model",
    "basePath": "/cds-au/v1"
  },
  "handlers": [
    {
      "type": "DataDefinitionHandler",
      "baseDirectory": "src/main/java",
      "modelTemplate": "java-holder-spring-models/model/model.vm",
      "enumTemplate": "java-holder-spring-models/model/enumcontainer.vm",
      "inlineEnumTemplate": "java-holder-spring-models/model/enum.vm",      
      "fileName": "${cds.getDefinitionName()}.java",
      "filePath": "${basePackagePath}",
      "additionalAttributes": {
        "packageName": "${basePackageName}"
      }
    },
    {
      "type": "GenericHandler",
      "baseDirectory": "src/main/java",
      "fileMapping": {
        "java-holder-spring-models/generic/pom.xml.vm": "../../../pom.xml"
      },
      "additionalAttributes": {
        "packageName": "${basePackageName}",
        "titleName": "Consumer Data Standards",
        "titleDescription": "API sets created by the Australian Consumer Data Standards to meet the needs of the Consumer Data Right",
        "licenseName": "MIT License",
        "licenseUrl": "https://opensource.org/licenses/MIT",
        "contactName": "Data61",
        "contactUrl": "https://consumerdatastandards.org.au",
        "contactEmail": "cdr-data61@csiro.au",
        "tosUrl": "https://www.csiro.au/en/About/Footer/Legal-notice"
      }
    }
  ],
  "typeMappings": {
    "String": "String",
    "List": "List<%s>",
    "DateTime": "OffsetDateTime",
    "Date": "LocalDate",
    "URI": "URI",
    "ExternalRef": "String",
    "ASCII": "String",
    "NaturalNumber": "Integer",    
    "Amount": "BigDecimal",
    "Currency": "String",
    "Rate": "BigDecimal",
    "PositiveInteger": "Integer"
  }
}