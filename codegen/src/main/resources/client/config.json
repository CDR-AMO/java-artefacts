{
  "globalVariables": {
    "basePackageName": "au.org.consumerdatastandards.client",
    "basePackagePath": "au/org/consumerdatastandards/client",
    "basePath": "/cds-au/v1"
  },
  "handlerConfigs": [
    {
      "type": "GenericHandler",
      "baseDirectory": "src/main/java",
      "fileMapping": {
        "client/generic/ApiCallback.java.vm": "${basePackagePath}/ApiCallback.java",
        "client/generic/ApiClient.java.vm": "${basePackagePath}/ApiClient.java",
        "client/generic/ApiException.java.vm": "${basePackagePath}/ApiException.java",
        "client/generic/ApiResponse.java.vm": "${basePackagePath}/ApiResponse.java",
        "client/generic/Configuration.java.vm": "${basePackagePath}/Configuration.java",
        "client/generic/JSON.java.vm": "${basePackagePath}/JSON.java",
        "client/generic/Pair.java.vm": "${basePackagePath}/Pair.java",
        "client/generic/ProgressRequestBody.java.vm": "${basePackagePath}/ProgressRequestBody.java",
        "client/generic/ProgressResponseBody.java.vm": "${basePackagePath}/ProgressResponseBody.java",
        "client/generic/pom.xml.vm": "../../../pom.xml"
      },
      "additionalAttributes": {
        "packageName": "${basePackageName}",
        "modelPackage": "${basePackageName}.model",
        "apiPackage": "${basePackageName}.api",
        "title": "Consumer Data Standards",
        "description": "API sets created by the Australian Consumer Data Standards to meet the needs of the Consumer Data Right",
        "licenseName": "MIT License",
        "licenseUrl": "https://opensource.org/licenses/MIT",
        "contactName": "Data61",
        "contactUrl": "https://consumerdatastandards.org.au",
        "contactEmail": "cdr-data61@csiro.au",
        "tosUrl": "https://www.csiro.au/en/About/Footer/Legal-notice"
      }
    },
    {
      "type": "DataDefinitionHandler",
      "baseDirectory": "src/main/java",
      "modelTemplate": "client/model/model.vm",
      "inlineEnumTemplate": "client/model/enum.vm",
      "enumTemplate": "client/model/enumcontainer.vm",
      "fileName": "${cds.getDefinitionName()}.java",
      "filePath": "${basePackagePath}/model",
      "additionalAttributes": {
        "packageName": "${basePackageName}.model"
      }
    },
    {
      "type": "SectionHandler",
      "baseDirectory": "src/main/java",
      "templateFile": "client/section/impl.vm",
      "fileName": "${cds.getInterfaceName()}.java",
      "filePath": "${basePackagePath}/api",
      "additionalAttributes": {
        "packageName": "${basePackageName}.api",
        "modelPackage": "${basePackageName}.model"
      }
    }
  ],
  "typeMappings": {
    "String": "String",
    "List": "List<%s>",
    "DateTime": "OffsetDateTime",
    "Date": "Date",
    "URI": "URI",
    "ExternalRef": "String",
    "ASCII": "String",
    "NaturalNumber": "Integer",
    "Amount": "BigDecimal",
    "Currency": "String",
    "Rate": "BigDecimal"
  }
}
